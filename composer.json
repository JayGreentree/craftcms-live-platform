{
    "require": {
        "adigital/help-links": "1.0.9",
        "craftcms/ckeditor": "1.3.0",
        "craftcms/cms": "3.8.4",
        "craftcms/redactor": "2.10.10",
        "focuslabllc/craft-cheat-sheet": "2.0.3",
        "nystudio107/craft-twigpack": "1.2.17",
        "nystudio107/craft-youtubeliveembed": "1.0.8",
        "putyourlightson/craft-sprig": "1.13.0",
        "solspace/craft-express-forms": "1.1.2",
        "vlucas/phpdotenv": "^5.4.0"
    },
    "require-dev": {
        "yiisoft/yii2-shell": "^2.0.3"
    },
    "autoload": {
        "psr-4": {
            "modules\\": "modules/"
        }
    },
    "config": {
        "allow-plugins": {
            "craftcms/plugin-installer": true,
            "yiisoft/yii2-composer": true
        },
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.2.5"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}